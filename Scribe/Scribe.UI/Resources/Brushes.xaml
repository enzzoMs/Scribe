<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <SolidColorBrush x:Key="PrimaryBrush" Color="#FF09ADAE"/>
    <SolidColorBrush x:Key="SurfaceBrush" Color="#FFFAFAF9"/>
    <SolidColorBrush x:Key="TextPrimaryBrush" Color="#FF232323"/>
    <SolidColorBrush x:Key="TextSecondaryBrush" Color="#FF606060"/>
    <SolidColorBrush x:Key="DividerBrush" Color="#FFD9D9D9"/>
    <SolidColorBrush x:Key="UnselectedBorderBrush" Color="#FFD9D9D9"/>
    <SolidColorBrush x:Key="SelectedBorderBrush" Color="#FFA4A4A3"/>
    <SolidColorBrush x:Key="DarkIconBrush" Color="#FF232323"/>
    <SolidColorBrush x:Key="LightIconBrush" Color="#FFFFFFFF"/>
    <Color x:Key="IconButtonRippleColor">#FFE0E0E0</Color>
</ResourceDictionary>