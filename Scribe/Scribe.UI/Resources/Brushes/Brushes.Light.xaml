<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <SolidColorBrush x:Key="Brush.Primary" Color="#FF09ADAE"/>
    <SolidColorBrush x:Key="Brush.Surface.Light" Color="#FFFAFAF9"/>
    <SolidColorBrush x:Key="Brush.Surface.Dark" Color="#FF232323"/>
    <SolidColorBrush x:Key="Brush.OnSurface.Dark" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="Brush.Warming" Color="#FFFF0000"/>
    <SolidColorBrush x:Key="Brush.Text.Primary" Color="#FF232323"/>
    <SolidColorBrush x:Key="Brush.Text.Secondary" Color="#FF606060"/>
    <SolidColorBrush x:Key="Brush.Divider" Color="#FFD9D9D9"/>
    <SolidColorBrush x:Key="Brush.Border.Unselected" Color="#FFD9D9D9"/>
    <SolidColorBrush x:Key="Brush.Border.Selected" Color="#FFA4A4A3"/>
    <SolidColorBrush x:Key="Brush.Icon" Color="#FF232323"/>
    <SolidColorBrush x:Key="Brush.BackgroundText" Color="#FF606060" Opacity="0.6"/>
    <SolidColorBrush x:Key="Brush.BackgroundIcon" Color="#FF606060" Opacity="0.5"/>
    <SolidColorBrush x:Key="Brush.Item.Selected.Background" Color="#FF232323"/>
    <SolidColorBrush x:Key="Brush.Item.Selected.Foreground" Color="#FFFFFFFF"/>

    <Color x:Key="Color.Divider">#FFD9D9D9</Color>
    <Color x:Key="Color.Text.Primary">#FF232323</Color>
</ResourceDictionary>